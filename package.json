{
  "name": "@sensethenlove/svelte-turnstile",
  "license": "MIT",
  "type": "module",
  "version": "1.0.4",
  "author": "Sense Then Love",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/sensethenlove/loop-backwards#readme",
  "description": "Helper functions & component to integrate svelte w/ an invisible cloudflare turnstile form validator",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sensethenlove/loop-backwards.git"
  },
  "bugs": {
    "url": "https://github.com/sensethenlove/loop-backwards/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "turnstile",
    "typescript",
    "cloudflare workers"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "npmDeploy": "npm publish --access public",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "publint": "^0.1.9",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
